	Number = {3},
	Owner = {D3M629},
	Pages = {486--496},
	Pii = {M900217-MCP200},
	Pmid = {20019053},
	Timestamp = {2014.03.24},
	Title = {DtaRefinery, a software tool for elimination of systematic errors from parent ion mass measurements in tandem mass spectra data sets.},
